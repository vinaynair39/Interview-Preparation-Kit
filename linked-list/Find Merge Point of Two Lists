
# Complete the findMergeNode function below.

#
# For your reference:
#
# SinglyLinkedListNode:
#     int data
#     SinglyLinkedListNode next
#
#
def findMergeNode(head1, head2):
    p1 = head1
    p2 = head2

    while p1 is not None and p2 is not None:
        if p1 == p2:
            return p1.data
        p1 = p1.next
        p2 = p2.next

    
    if p1 is None:
        p1 = head1
        while p2 is not None:
            p2 = p2.next
            p1 = p1.next
        return p1.data
    elif p2 is None:
        p2 = head2
        while p1 is not None:
            p1 = p1.next
            p2 = p2.next
        return p2.data


